# Native Development Environment for AstraDraw Backend

# Domain
APP_DOMAIN=draw.local
APP_PROTOCOL=https
APP_URL=https://draw.local

# Database (PostgreSQL in Docker, exposed on localhost:5432)
DATABASE_URL=postgresql://excalidraw:your_secure_postgres_password@localhost:5432/excalidraw?schema=public

# S3/MinIO (in Docker, exposed on localhost:9000)
STORAGE_BACKEND=s3
S3_ENDPOINT=http://localhost:9000
S3_BUCKET=excalidraw
S3_REGION=us-east-1
S3_FORCE_PATH_STYLE=true
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=8vjtzDctc1bEOyfd0FOLySnb89wYFRPn

# Authentication
JWT_SECRET=dev-secret-change-in-production
ENABLE_LOCAL_AUTH=true
ENABLE_REGISTRATION=true
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin
ADMIN_EMAIL=admin@localhost
SUPERADMIN_EMAILS=admin@localhost

# Service port
PORT=8080

# Debug
DEBUG_NAVIGATION=false
LOG_LEVEL=log
